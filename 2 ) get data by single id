from fastapi import FastAPI

app = FastAPI()


@app.get("/show/{id}")
def show(id):
    return {"data":id}
    
    
    
    
+++++++++++++++++++++++++++++++++++++++++++

@app.get("/blog/{id}/comments")
def comments(id):
    return {"data":{"1",'2'}}


_____ OUTPUT ______

{
"data": [
"2",
"1"
]
}


+++++++++++++++++++++++++++++ Pass only integer +++++++++++++++++++++

@app.get("/show/{id}")
def show(id:int):
    return {"data":id}
